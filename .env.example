# Database
# Use a plain Postgres URL so you can also run: psql "$DATABASE_URL"
# app/db.py upgrades this internally to postgresql+psycopg for SQLAlchemy.
DATABASE_URL=postgresql://user:pass@localhost:5432/nrl_edge

# Discord
DISCORD_WEBHOOK_URL=

# Execution
DEPLOY_SEASON=2026
DEPLOY_ROUND=1

# Dry-run
# DRY_RUN=1 still persists predictions + slips (status=dry_run) for HUD/PDF debugging.
DRY_RUN=0
# DRY_NOTIFY=1 will send Discord notifications even in dry-run mode.
DRY_NOTIFY=0

# Model
MODEL_VERSION=v2026-02-xgb-v1

# Playwright (referee scraper)
PLAYWRIGHT_BROWSERS_PATH=0
